import time
import threading
message = 0

print("set time")
def settime(message):
  message =int(input())
  for i in range (message):
    
    print(threading.currentThread().getName(), message-i)
    time.sleep(1)
  print("done")
 
if __name__ == '__main__':
    for i in range(3):
        my_thread = threading.Thread(target=settime, args=(i,))
        my_thread.start() 
